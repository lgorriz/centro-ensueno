{
  "name": "centro-dia-ensueno",
  "version": "1.1.0",
  "homepage": "https://lgorriz.github.io",
  "description": "Centro de día Ensueño",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgorriz/centro-ensueno.git"
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgorriz/centro-ensueno.git/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css --source-map true",
    "predeploy": "react-scripts build && gulp licenses",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "formik": "^2.2.9",
    "fslightbox-react": "^1.7.4",
    "headroom.js": "0.11.0",
    "moment": "2.24.0",
    "nouislider": "14.1.1",
    "react": "16.12.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.12.0",
    "react-images": "^1.2.0-beta.7",
    "react-photo-gallery": "^8.0.0",
    "react-router": "5.1.2",
    "react-router-dom": "^6.10.0",
    "react-scripts": "3.4.1",
    "react-select": "^5.2.1",
    "reactstrap": "8.4.1",
    "sass": "^1.58.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.19",
    "eslint-plugin-flowtype": "3.13.0",
    "gh-pages": "^5.0.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "jquery": "3.4.1",
    "typescript": "3.7.5"
  }
